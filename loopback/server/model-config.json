{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mysqlDs",
    "public": true,
    "options": {
      "emailVerificationRequired": false
    }
  },
  "user": {
    "dataSource": "mysqlDs",
    "public": true,
    "options": {
      "emailVerificationRequired": false
    }
  },
  "AccessToken": {
    "dataSource": "mysqlDs",
    "public": false
  },
  "ACL": {
    "dataSource": "mysqlDs",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysqlDs",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Email": {
      "dataSource": "mailgun",
      "public": false
  },
  "Role": {
    "dataSource": "mysqlDs",
    "public": false
  },
  "vote": {
    "dataSource": "mysqlDs",
    "public": true
  },
  "term": {
    "dataSource": "mysqlDs",
    "public": true
  },
  "language": {
    "dataSource": "mysqlDs",
    "public": true
  }
}
